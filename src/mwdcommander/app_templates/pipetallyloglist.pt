<div tal:on-error="string: No Pipe Tallies Available!">
  <span tal:repeat="log view/context/pipetallies/items">
    <div tal:attributes="id python: 'n'+log.id">
      <a tal:content="log/name" tal:attributes="href python: view.__parent__.application_url('pipetallies') + '/' + unicode(log.id)"/>
      <div tal:attributes="logid python: 'n'+log.id" id="delete-pipe-tally-log">Delete</div>
    </div>
  </span>
</div>

